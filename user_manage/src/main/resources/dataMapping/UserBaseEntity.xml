<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.smart.entity.UserBaseEntity">
        <table name="user_base" schema="user_base" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" length="32"/>
            </id>
            <basic name="name">
                <column name="name" length="45"/>
            </basic>
            <basic name="userName">
                <column name="user_name" length="45"/>
            </basic>
            <basic name="createTime">
                <column name="create_time" precision="-1"/>
            </basic>
            <basic name="brithday">
                <column name="brithday" precision="-1"/>
            </basic>
            <basic name="email">
                <column name="email" length="70"/>
            </basic>
            <basic name="numberPhone">
                <column name="number_phone" length="11"/>
            </basic>
            <basic name="identVard">
                <column name="ident_vard" length="18"/>
            </basic>
            <basic name="sex">
                <column name="sex"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
